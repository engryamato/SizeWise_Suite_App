{
  "name": "sizewise-suite-nextjs-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js frontend with glassmorphism UI for SizeWise Suite HVAC platform",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --config=tests/config/jest.config.ts",
    "test:watch": "jest --config=tests/config/jest.config.ts --watch",
    "test:coverage": "jest --config=tests/config/jest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:critical": "playwright test critical-workflows.spec.ts",
    "test:e2e:performance": "playwright test performance-workflows.spec.ts",
    "test:accessibility": "jest --config=tests/config/jest.config.ts tests/accessibility/",
    "test:accessibility:watch": "jest --config=tests/config/jest.config.ts tests/accessibility/ --watch",
    "test:accessibility:coverage": "jest --config=tests/config/jest.config.ts tests/accessibility/ --coverage",
    "test:accessibility:ci": "jest --config=tests/config/jest.config.ts tests/accessibility/ --ci --coverage --watchAll=false --passWithNoTests",
    "test:wcag": "jest --config=tests/config/jest.config.ts tests/accessibility/automated-wcag-compliance.test.ts",
    "test:wcag:forms": "jest --config=tests/config/jest.config.ts tests/accessibility/form-accessibility.test.ts",
    "test:wcag:navigation": "jest --config=tests/config/jest.config.ts tests/accessibility/navigation-accessibility.test.ts",
    "test:load": "playwright test load-testing.spec.ts",
    "test:load:light": "playwright test load-testing.spec.ts -g \"light load\"",
    "test:load:medium": "playwright test load-testing.spec.ts -g \"medium load\"",
    "test:load:stress": "playwright test load-testing.spec.ts -g \"stress test\"",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:comprehensive": "npm run test:e2e:critical && npm run test:e2e:performance && npm run test:visual && npm run test:load",
    "test:visual:headed": "playwright test --config=playwright-visual.config.ts --headed",
    "playwright:install": "playwright install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@sentry/nextjs": "^9.42.0",
    "@types/qrcode.react": "^1.0.5",
    "@types/three": "^0.178.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "framer-motion": "^12.23.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "konva": "^9.2.0",
    "lucide-react": "^0.468.0",
    "next": "^15.4.2",
    "next-pwa": "^5.6.0",
    "ogl": "^1.0.11",
    "onnxruntime-web": "^1.22.0",
    "pdfjs-dist": "^5.3.93",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.7",
    "react-pdf": "^10.0.1",
    "recharts": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0",
    "use-debounce": "^10.0.0",
    "zod": "^4.0.14",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@21st-extension/toolbar-next": "^0.5.14",
    "@axe-core/react": "^4.10.2",
    "@jest/globals": "^30.0.5",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.54.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.17.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^12.2.0",
    "cross-env": "^10.0.0",
    "cypress": "^13.6.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.2",
    "fake-indexeddb": "^6.0.1",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-transform-css": "^6.0.3",
    "jest-transform-file": "^1.1.1",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "keywords": [
    "nextjs",
    "glassmorphism",
    "hvac",
    "typescript",
    "tailwindcss"
  ],
  "author": "SizeWise Suite Team",
  "license": "MIT"
}
