  "calculation_methods": {
    "density_correction": {
      "formula": "ρ_corrected = ρ_base × (T_base/T_actual) × (P_actual/P_base) × humidity_factor",
      "description": "Corrects density for temperature, pressure, and humidity",
      "variables": {
        "ρ_base": "Base density at standard conditions (lb/ft³)",
        "T_base": "Base temperature (°R = °F + 459.67)",
        "T_actual": "Actual temperature (°R)",
        "P_base": "Base pressure (in Hg)",
        "P_actual": "Actual pressure (in Hg)",
        "humidity_factor": "Humidity correction factor"
      }
    },
    "viscosity_correction": {
      "formula": "μ_corrected = μ_base × (T_actual/T_base)^0.7",
      "description": "Corrects viscosity for temperature using Sutherland's law approximation",
      "variables": {
        "μ_base": "Base viscosity at standard conditions (lb/(ft·s))",
        "T_base": "Base temperature (°R)",
        "T_actual": "Actual temperature (°R)"
      }
    },
    "interpolation": {
      "method": "linear",
      "description": "Linear interpolation recommended for intermediate values",
      "formula": "y = y1 + (x - x1) × (y2 - y1) / (x2 - x1)"
    }
  },
